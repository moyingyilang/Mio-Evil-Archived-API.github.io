<!DOCTYPE html>
<html>
<head>
  <title>Random Image API</title>
  <script>
    fetch('/scripts/image-list.js')
      .then(response => response.text())
      .then(scriptText => {
        // 解析列表
        const images = eval(scriptText.replace('const images =', '').replace(';', ''));
        // 随机选择
        const randomImage = images[Math.floor(Math.random() * images.length)];
        // 重定向
        window.location.href = randomImage;
      })
      .catch(error => {
        document.body.innerHTML = `<h1>Error loading images: ${error}</h1>`;
      });
  </script>
</head>
<body>
  <h1>Redirecting to random image...</h1>
</body>
</html>
